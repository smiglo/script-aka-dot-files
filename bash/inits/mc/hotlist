  ENTRY "TOM-WORK" URL "/media/$USER/TOM-data"
ENDGROUP
GROUP "My"
  ENTRY "Documents" URL "/$HOME/Documents/priv"
  ENTRY "Scripts" URL "/$MY_PROJ_PATH/scripts"
  ENTRY "Vim" URL "/$MY_PROJ_PATH/vim"
ENDGROUP
